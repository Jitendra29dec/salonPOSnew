<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#F4F3F3"
    android:gravity="center"
    android:orientation="vertical">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_22sdp"
            android:background="@drawable/header_bg">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/_8sdp"
                android:layout_marginEnd="@dimen/_8sdp"
                android:text="Book Appointment"
                android:textColor="@color/white"
                android:textSize="@dimen/_8sdp" />

            <ImageView
                android:id="@+id/imgClose"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/_8sdp"
                android:layout_marginEnd="@dimen/_8sdp"
                android:padding="@dimen/_3sdp"
                android:src="@drawable/ic_close_white" />
        </RelativeLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:fillViewport="true"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/_3sdp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Services *"
                        android:textColor="#000"
                        android:textSize="@dimen/_6sdp" />

                    <Spinner
                        android:id="@+id/spinService"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_18sdp"
                        android:background="@drawable/spinner_bg" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:text="Stylist *"
                        android:textColor="#000"
                        android:textSize="@dimen/_6sdp" />

                    <Spinner
                        android:id="@+id/spinStyler"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_18sdp"
                        android:background="@drawable/spinner_bg" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Date *"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                            <TextView
                                android:id="@+id/txtDate"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_18sdp"
                                android:background="@drawable/rounded_corner_grey_bg"
                                android:hint="DD/MM/YYYY"
                                android:gravity="center_vertical"
                                android:paddingStart="@dimen/_5sdp"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Start Time *"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                            <TextView
                                android:id="@+id/txtTime"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_18sdp"
                                android:background="@drawable/edit_duration_bg"
                                android:drawableEnd="@drawable/ic_start_time"
                                android:gravity="center_vertical"
                                android:hint="HH:MM"
                                android:paddingStart="@dimen/_5sdp"
                                android:paddingEnd="@dimen/_4sdp"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Price *"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                            <TextView
                                android:id="@+id/txtPrice"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_18sdp"
                                android:background="@drawable/edit_price_bg"
                                android:drawableStart="@drawable/ic_dollar"
                                android:drawablePadding="@dimen/_8sdp"
                                android:gravity="center_vertical"
                                android:hint="0"
                                android:paddingStart="@dimen/_4sdp"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Duration *"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                            <TextView
                                android:id="@+id/txtDuration"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_18sdp"
                                android:background="@drawable/edit_duration_bg"
                                android:drawableEnd="@drawable/ic_time_duration"
                                android:gravity="center_vertical"
                                android:hint="0"
                                android:paddingStart="@dimen/_5sdp"
                                android:paddingEnd="@dimen/_4sdp"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Note for this appointment :"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_6sdp" />

                    <EditText
                        android:id="@+id/editCustomerNote"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_100sdp"
                        android:background="@drawable/rounded_corner_grey_bg"
                        android:gravity="start"
                        android:hint="Enter Note Here..."
                        android:padding="@dimen/_3sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_6sdp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginBottom="@dimen/_11sdp"
                        android:visibility="gone"
                        android:background="@color/very_light_grey" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Repeat:"
                        android:visibility="gone"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_6sdp" />

                    <RadioGroup
                        android:id="@+id/rGroupRepeat"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_18sdp"
                        android:visibility="gone"
                        android:background="@drawable/rounded_corner_grey_bg"
                        android:orientation="horizontal"
                        android:padding="@dimen/_2sdp">

                        <RadioButton
                            android:id="@+id/rBtnOff"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/repeat_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:text="Off"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                        <RadioButton
                            android:id="@+id/rBtnDaily"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/repeat_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:text="Daily"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                        <RadioButton
                            android:id="@+id/rBtnWeekly"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/repeat_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:text="Weekly"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                        <RadioButton
                            android:id="@+id/rBtnMonthly"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/repeat_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:text="Monthly"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                        <RadioButton
                            android:id="@+id/rBtnYearly"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/repeat_selector"
                            android:button="@null"
                            android:gravity="center"
                            android:text="Yearly"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                    </RadioGroup>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_3sdp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Customer *"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_6sdp" />

                    <AutoCompleteTextView
                        android:id="@+id/appView"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_18sdp"
                        android:background="@drawable/spinner_bg"
                        android:maxLines="1"
                        android:paddingStart="@dimen/_4sdp"
                        android:paddingEnd="@dimen/_4sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_6sdp" />

                    <TextView
                        android:id="@+id/addDialogButton"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:drawableStart="@drawable/add_icon_blue"
                        android:gravity="center_vertical"
                        android:text="@string/addnewcustomer"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_6sdp" />


                    <LinearLayout
                        android:id="@+id/customerLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imgCustomerProfile"
                            android:layout_width="@dimen/_35sdp"
                            android:layout_height="@dimen/_25sdp"
                            android:background="@drawable/rounded_corner_grey_bg"
                            android:src="@drawable/default_user" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_3sdp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtCustomerName"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_12sdp"
                                android:gravity="center_vertical"
                                android:text="Name : "
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                            <TextView
                                android:id="@+id/txtCustomerMobile"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_12sdp"
                                android:gravity="center_vertical"
                                android:text="Mobile : "
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                            <TextView
                                android:id="@+id/txtCustomerEmail"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_12sdp"
                                android:gravity="center_vertical"
                                android:text="Email : "
                                android:textColor="@color/black"
                                android:textSize="@dimen/_6sdp" />

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_26sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:background="@color/very_light_grey"
                            android:gravity="center"
                            android:text="Notes"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:background="@color/very_light_grey"
                            android:gravity="center"
                            android:text="History"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_6sdp" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/very_light_grey" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/btnAddService"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_20sdp"
                        android:layout_margin="@dimen/_5sdp"
                        android:background="@drawable/btn_blue"
                        android:drawableStart="@drawable/add_icon"
                        android:paddingStart="@dimen/_3sdp"
                        android:paddingEnd="@dimen/_5sdp"
                        android:text="Add Service"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_6sdp" />

                    <Button
                        android:id="@+id/btnAddGroupService"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_20sdp"
                        android:layout_margin="@dimen/_5sdp"
                        android:background="@drawable/btn_sky_blue"
                        android:drawableStart="@drawable/add_icon"
                        android:paddingStart="@dimen/_3sdp"
                        android:paddingEnd="@dimen/_5sdp"
                        android:text="Add Group Service"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_6sdp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="end"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/btnCancel"
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_20sdp"
                        android:layout_margin="@dimen/_5sdp"
                        android:background="@drawable/rounded_corner_grey_bg"
                        android:text="Cancel"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_6sdp" />

                    <Button
                        android:id="@+id/btnBookAppointment"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_20sdp"
                        android:layout_margin="@dimen/_5sdp"
                        android:background="@drawable/btn_sky_blue"
                        android:paddingStart="@dimen/_5sdp"
                        android:paddingEnd="@dimen/_5sdp"
                        android:text="Book Appointment"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_6sdp" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>


